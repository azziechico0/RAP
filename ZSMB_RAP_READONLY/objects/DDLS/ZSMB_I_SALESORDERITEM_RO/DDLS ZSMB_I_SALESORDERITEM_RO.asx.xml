<SOURCE>@AbapCatalog.sqlViewName: 'ZSMB_I_SOI'
@AbapCatalog.compiler.compareFilter: true
@AbapCatalog.preserveKey: true
@AccessControl.authorizationCheck: #NOT_REQUIRED
@EndUserText.label: 'Read Only SalesOrder Item'
@VDM.viewType: #BASIC

define view ZSMB_I_SALESORDERITEM_RO 
   as select from snwd_so_i as SalesOrderItem 
  
   association [1..1] to snwd_pd          as _Product          on $projection.Product = _Product.node_key   
{
    key SalesOrderItem.node_key     as Node_Key,      
        SalesOrderItem.parent_key   as Parent_Key,     
        SalesOrderItem.so_item_pos  as SalesOrderItemID,       
        SalesOrderItem.product_guid as Product,      
        @Semantics.currencyCode: true        
        SalesOrderItem.currency_code    as CurrencyCode,  
        @Semantics.amount.currencyCode: 'CurrencyCode'             
        SalesOrderItem.gross_amount     as GrossAmount,  
        @Semantics.amount.currencyCode: 'CurrencyCode'         
        SalesOrderItem.net_amount       as NetAmount,       
        @Semantics.amount.currencyCode: 'CurrencyCode'           
        SalesOrderItem.tax_amount       as TaxAmount,

        /* Associations */   
        _Product  
}
