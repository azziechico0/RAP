define view entity ZSMB_C_SALESORDERITEM_RO 
   as projection on ZSMB_I_SALESORDERITEM_RO as SalesOrderItem  {

      key Node_Key,      
      Parent_Key,
      @Search: { defaultSearchElement: true, fuzzinessThreshold: 0.7 }
      @Consumption.valueHelpDefinition: [{ entity: { name: 'ZSMB_I_SALESORDERITEM_RO', element: 'SalesOrderItemID' } }]      
      SalesOrderItemID,
      _Product.product_id as ProductId,      
      CurrencyCode,
      GrossAmount,
      NetAmount,           
      TaxAmount,  
      
      /* Exposing value via associations */        
      _SalesOrder: redirected to parent ZSMB_C_SALESORDER_RO   
}
