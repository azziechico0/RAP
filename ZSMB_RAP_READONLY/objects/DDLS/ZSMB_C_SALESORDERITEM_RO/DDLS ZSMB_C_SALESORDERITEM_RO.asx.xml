<SOURCE>@AbapCatalog.sqlViewName: 'ZSMB_C_SOI'
@AbapCatalog.compiler.compareFilter: true
@AbapCatalog.preserveKey: true
@AccessControl.authorizationCheck: #NOT_REQUIRED
@EndUserText.label: 'Read Only SalesOrder Item Consumption'

@Search.searchable: true

@UI.headerInfo: { typeName       : 'Sales Order Item List', 
                  typeNamePlural : 'Sales Orders Item List' }

define view ZSMB_C_SALESORDERITEM_RO 
   as select from ZSMB_I_SALESORDERITEM_RO as SalesOrderItem {
   
     @UI.facet: [{ id              : 'idSalesOrderItem',
                   type            : #IDENTIFICATION_REFERENCE,                         
                   label           : 'SalesOrderItem(Identification)',
                   position        : 10 }]
//                 { id: 'idLineItemProduct',
//                   type            : #LINEITEM_REFERENCE,
//                   label           : 'Product (LineItem_Reference)',
//                   position        : 20,
//                   targetQualifier : 'fgProductId',
//                   targetElement   : '_Product' }]    
                                   
      @UI.hidden: true
      key SalesOrderItem.Node_Key,
      @UI.hidden: true      
      SalesOrderItem.Parent_Key as Parent_Key,
      @Search.defaultSearchElement: true
      @Search.fuzzinessThreshold: 0.7
      @UI.lineItem: { qualifier: 'fgSOItem', position: 10, label: 'Sales Order Item' }
      @UI.identification: { position: 10, label: 'Sales Order Item' }  
      SalesOrderItem.SalesOrderItemID,
      @UI.lineItem: { qualifier: 'fgSOItem', position: 20, label: 'Product ID' }
      @UI.identification: { position: 20, label: 'Product ID' }         
      SalesOrderItem._Product.product_id,  
      SalesOrderItem.CurrencyCode as Currency,  
      @UI.lineItem: { qualifier: 'fgSOItem', position: 30, label: 'Gross Amount' }
      @UI.identification: { position: 60, label: 'Gross Amount' }      
      SalesOrderItem.GrossAmount,  
      @UI.lineItem: { qualifier: 'fgSOItem', position: 40, label: 'Net Amount' }
      @UI.identification: { position: 30, label: 'Net Amount' }  
      SalesOrderItem.NetAmount,       
      @UI.lineItem: { qualifier: 'fgSOItem', position: 50, label: 'Tax Amount' }
      @UI.identification: { position: 50, label: 'Tax Amount' }     
      SalesOrderItem.TaxAmount
//      SalesOrderItem.Product,
      
//       /* Exposing value via associations */
//      @UI.lineItem: { position: 10, qualifier: 'fgProductId' }                
//      _Product     
}
