<SOURCE>@AbapCatalog.sqlViewName: 'ZSMB_I_BP_RO'
@AbapCatalog.compiler.compareFilter: true
@AbapCatalog.preserveKey: true
@AccessControl.authorizationCheck: #NOT_REQUIRED
@EndUserText.label: 'Business Partner View - CDS Data Model'

@Search.searchable: true

define view ZSMB_I_BUSINESSPARTNER_RO
  as select from snwd_bpa as BusinessPartner

  association [0..1] to snwd_ad as _Address on $projection.AddressGuid = _Address.node_key

{
      @ObjectModel.text.element: ['CompanyName']
  key BusinessPartner.node_key        as NodeKey,
      BusinessPartner.bp_id           as BP_Id,
      @Search.defaultSearchElement: true
      @Search.fuzzinessThreshold: 0.8
      @Semantics.text: true
      BusinessPartner.company_name    as CompanyName,
      @Search.defaultSearchElement: true
      @Search.fuzzinessThreshold: 0.8  
      BusinessPartner.bp_role         as Role,
      BusinessPartner.email_address   as Email,
      BusinessPartner.phone_number    as PhoneNumber,
      BusinessPartner.fax_number      as FaxNumber,
      BusinessPartner.web_address     as WebAddress,
      BusinessPartner.address_guid    as AddressGuid,
      BusinessPartner.legal_form      as LegalForm,
      BusinessPartner.created_by      as CreatedBy,
      BusinessPartner.created_at      as CreatedAt,
      BusinessPartner.changed_by      as ChangedBy,
      BusinessPartner.changed_at      as ChangedAt,
      BusinessPartner.currency_code   as CurrencyCode,
      BusinessPartner.dummy_field_bpa as DummyFieldBPA,
      BusinessPartner.approval_status as ApprovalStatus,

      /* Associations */
      _Address
}
