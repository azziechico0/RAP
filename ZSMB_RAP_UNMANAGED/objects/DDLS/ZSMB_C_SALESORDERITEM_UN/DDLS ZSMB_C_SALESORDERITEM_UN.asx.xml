<SOURCE>@EndUserText.label: 'Read Only SalesOrder Item Consumption'
@AccessControl.authorizationCheck: #NOT_REQUIRED

@Metadata.allowExtensions: true

@Search.searchable: false

define view entity ZSMB_C_SALESORDERITEM_UN 
   as projection on ZSMB_I_SALESORDERITEM_UN as SalesOrderItem {
      key Node_Key,      
      Parent_Key,
      @Search: { defaultSearchElement: true, fuzzinessThreshold: 0.7 }
      @Consumption.valueHelpDefinition: [{ entity: { name: 'ZSMB_I_SALESORDERITEM_UN', element: 'SalesOrderItemID' } }]      
      SalesOrderItemID,
      @Search.defaultSearchElement: true      
      @Consumption.valueHelpDefinition: [ { entity: { name: 'SEPM_I_Product', element: 'ProductUUID' } } ]
      @ObjectModel.text.element: ['ProductName']
      Product as ProductNodeKey,      
      _Product.Product    as ProductName,       
      CurrencyCode,
      GrossAmount,
      NetAmount,           
      TaxAmount,  
      
      /* Exposing value via associations */        
      _SalesOrder: redirected to parent ZSMB_C_SALESORDER_UN   
}
